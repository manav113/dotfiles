% youtube-dl

# Download mp3 best possible quality
youtube-dl -f bestaudio --extract-audio --audio-format mp3 --audio-quality 0 '<video_url>'

# Download m4a best quality, file size optimized
youtube-dl -f bestaudio[ext=m4a] --embed-thumbnail --add-metadata '<video_url>'

# Download mp3 best possible quality from a file
youtube-dl -f bestaudio --extract-audio --audio-format mp3 --audio-quality 0  --batch-file='<link_list_file>'
