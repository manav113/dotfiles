# This is Git's per-user configuration file.
[user]
authorName = 
email = 

[init]
	defaultBranch = main

[pull]
	rebase = true

[push]
	autoSetupRemote = true

[http]
	proxy = 
	sslverify = false

[core]
	editor = code --wait
